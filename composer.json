{
    "name": "popphp/pop-dir",
    "description": "Pop Directory Component for Pop PHP Framework",
    "keywords": [
        "directory",
        "directory traversing",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "New BSD",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Dir\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Dir\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
